import discord
import os

client = discord.Client()

@client.event
async def on_ready():
    print('Kardien Online')

@client.event
async def on_message(message):
    if message.content.startswith("!ip"):
        await message.channel.send("Kardien.Kro.Kr")

access_token = os.environ["BOT_TOKEN"]
client.run(access_token)
